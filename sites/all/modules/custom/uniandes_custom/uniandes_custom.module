function configurar_bloques(){
   module_load_include('inc', 'block', 'block.admin');
  $theme_key = "uniandes_base";

  $blocks = block_admin_display_prepare_blocks($theme_key);

    foreach( $blocks as $block ){

    $region = null;
    $theme = "uniandes_base";
    
     switch( $block[ "info" ] ){

        case "Main page content":
          _activate_block("system", "main", "content", $theme, "", 1, 0);
        break;

        case "View: Slider Home: Block":
          _activate_block("views", "slider_home-block", "content", $theme, "<front>", 1, 0);
        break;

        case "View: Vista Anuncios Home: Block":
          _activate_block("views", "vista_noticias_home-block", "content", $theme, "<front>", 1 , 1);
        break;      

        case "View: Vista Eventos Home: Block":
          _activate_block("views", "vista_eventos_home-block", "content", $theme, "<front>", 1, 2);
        break;      

        case "View: Vista Ranking Home":
          _activate_block("views", "vista_ranking_home-block", "content", $theme, "<front>", 1, 3);
        break;

        case "View: Vista Noticias Multimedia Home":
          _activate_block("views", "d9b032d87d4e5d8b62a27ad010a5f55a", "content", $theme, "<front>", 1, 4);
        break;

        case "View: Vista Publicaciones Home: Block":          
          _activate_block("views", "vista_publicaciones_home-block", "content", $theme, "<front>", 1, 5);
        break;

        case "View: Vista lista maestra 11":
          _activate_block("views", "vista_lista_maestra_11-block_1", "content", $theme, "node/5830", 1, 6);
        break;

        case "View: Vista lista maestra 11":
          _activate_block("views", "vista_lista_maestra_11-block_1", "content", $theme, "node/5830", 1, 7);
        break;

        case "View: Footer: Block":        
          _activate_block("views", "footer-block", "footer", $theme, "", 1, -5);
        break;

       }

    }
    $_SESSION['install-step']=4;
}

