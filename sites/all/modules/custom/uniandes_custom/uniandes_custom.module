function uniandes_custom_node_view($node, $view_mode, $langcode){

  if( $node->type == "especiales" ){

    $node->field_visit_count["und"][0]["value"] = $node->field_visit_count["und"][0]["value"] + 1 ;
    node_save( $node );

  }

}

function _activate_block($module, $block, $region, $theme, $pages, $visibility, $weight) {

  db_merge('block')
  ->key(array('theme' => $theme, 'delta' => $block, 'module' => $module))
  ->fields(array(
    'region' => ($region == BLOCK_REGION_NONE ? '' : $region),
    'pages' => trim($pages),
    'status' => (int) ($region != BLOCK_REGION_NONE),
    'title' =>'<none>',
    'visibility' => $visibility,
    'weight' => $weight
    ))
  ->execute();
}

